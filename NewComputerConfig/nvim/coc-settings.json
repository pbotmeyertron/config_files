{
    "suggest.labelMaxLength": 50,
    "suggest.enablePreview": true,
    "suggest.snippetIndicator": " \ue796",
    "suggest.timeout": 5000,
    "suggest.triggerCompletionWait": 30,
    "suggest.removeDuplicateItems": true,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "diagnostic.level": "hint",
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic.virtualTextPrefix": " ▸ ",
    "diagnostic.errorSign": "\uf65b",
    "diagnostic.warningSign": "\uf525",
    "diagnostic.infoSign": "\uf7fb",
    "diagnostic.hintSign": "\uf7d6",
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck",
        "zsh": "shellcheck"
    },
    "diagnostic-languageserver.formatFiletypes": {
        "sh": "shfmt",
        "zsh": "shfmt"
    },
    "tree.closedIcon": "",
    "tree.openedIcon": "",
    "tree.key.toggle": "o",
    "tree.key.collapseAll": "O",
    "tree.key.actions": "<tab>",
    "tree.key.toggleSelection": "s",
    "outline.keepWindow": true,
    "signature.triggerSignatureWait": 300,
    "codeLens.enable": true,
    "codeLens.separator": "▸",
    "list.indicator": "❯",
    "list.source.grep.args": [
        "--hidden",
        "--no-ignore"
    ],
    "list.insertMappings": {
        "<C-p>": "action:preview",
        "<C-j>": "do:next",
        "<C-k>": "do:previous",
        "<C-s>": "do:toggle",
        "<C-f>": "do:switch",
        "<Down>": "prompt:next",
        "<Up>": "prompt:previous",
        "<C-v>": "prompt:paste"
    },
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.formatOnType": true,
    "git.changedSign.text": "▌",
    "git.addedSign.text": "▌",
    "git.removedSign.text": "▁",
    "git.changeRemovedSign.text": "▁",
    "git.topRemovedSign.text": "▔",
    "git.changedDecorator": " ",
    "git.stagedDecorator": " ",
    "git.untrackedDecorator": " ✩",
    "git.conflictedDecorator": "⇡⇣",
    "git.virtualTextPrefix": " \ue729 ",
    "git.addGBlameToVirtualText": false,
    "git.blameUseRealTime": false,
    "project.rootPatterns": [".git", ".svn", ".root", ".project", ".hg"],
    "snippets.ultisnips.enable": false,
    "snippets.snipmate.enable": false,
    "snippets.shortcut": "S",
    "snippets.priority": 99,
    "snippets.extends": {
        "cpp": ["c"],
        "javascriptreact": ["javascript"],
        "typescript": ["javascript"]
    },
    "suggest.languageSourcePriority": 98,
    "coc.source.file.shortcut": "F",
    "coc.source.file.priority": 89,
    "coc.source.file.ignoreHidden": false,
    "coc.source.around.shortcut": "A",
    "coc.source.around.priority": 88,
    "coc.source.buffer.shortcut": "B",
    "coc.source.buffer.priority": 87,
    "coc.source.buffer.ignoreGitignore": false,
    "coc.source.syntax.shortcut": "X",
    "coc.source.syntax.priority": 79,
    "coc.source.tag.shortcut": "T",
    "coc.source.tag.priority": 78,
    "coc.source.dictionary.shortcut": "D",
    "coc.source.dictionary.priority": 9,
    "coc.source.dictionary.filetypes": ["markdown", "text", "help", "gitcommit", "snippets"],
    "coc.source.emoji.shortcut": "E",
    "coc.source.emoji.priority": 8,
    "coc.source.emoji.filetypes": ["markdown", "text"],
    "colors.filetypes": ["*"],
    "highlight.colors.enable": true,
    "highlight.colorNames.enable": false,
    "highlight.document.enable": true,
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact"
    },
    "imselect.defaultInput": "com.apple.keylayout.ABC",
    "prettier.disableLanguages": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue",
        "json"
    ],
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 30,
    "explorer.focus": false,
    "explorer.buffer.tabOnly": true,
    "explorer.keyMappingMode": "none",
    "explorer.keyMappings.global": {
        "s": "toggleSelection",
        "<tab>": "actionMenu",

        "h": "indentPrev",
        "l": "indentNext",
        "o": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
        "<cr>": "open",
        "t": "open:tab",
        "<bs>": "gotoParent",

        "Y": "copyFilepath",
        "y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",

        "n": "addFile",
        "N": "addDirectory",
        "r": "rename",

        ".": "toggleHidden",
        "R": "refresh",
        "<C-p>": "preview",

        "?": "help",
        "<esc>": "esc",
        "e": "systemExecute",

        "f": "searchRecursive",

        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",

        "gk": "gitPrev",
        "gj": "gitNext",
        "ga": "gitStage",
        "gu": "gitUnstage"
    },
    "clangd.enabled": true,
    "clangd.fallbackFlags": ["-std=c++20"],
    "clangd.arguments": ["--clang-tidy", "--completion-style=detailed", "--fallback-style=google", "--header-insertion=iwyu", "--suggest-missing-includes"],
    "cmake.lsp.enable": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.inlayHints.enable": true,
    "rust-analyzer.inlayHints.typeHintsSeparator": "‣ ",
    "rust-analyzer.inlayHints.chainingHintsSeparator": "‣ ",
    "rust-analyzer.inlayHints.refreshOnInsertMode": true,
    "rust-analyzer.lens.enable": false,
    "python.analysis.typeCheckingMode": "basic",
    "lua.installPreReleases": true,
    "Lua.workspace.library": {
        "/opt/local/share/nvim/runtime/lua": true,
        "/opt/local/share/nvim/runtime/lua/vim": true,
        "/opt/local/share/nvim/runtime/lua/vim/lsp": true,
        "/usr/share/nvim/runtime/lua": true,
        "/usr/share/nvim/runtime/lua/vim": true,
        "/usr/share/nvim/runtime/lua/vim/lsp": true
    },
    "Lua.diagnostics.globals": ["vim"],
    "texlab.path": "texlab",
    "texlab.build.onSave": true,
    "sql.database": "mysql",
    "markdown-preview-enhanced.previewTheme": "one-dark.css",
    "markdown-preview-enhanced.enableScriptExecution": true,
    "markdownlint.config": {
        "MD013": false,
        "MD036": false,
        "MD041": false,
        "MD014": false
    },
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "git-commit",
        "latex",
        "plaintext",
        "text"
    ],
    "languageserver": {
      "haskell": {
        "command": "haskell-language-server-wrapper",
        "args": ["--lsp"],
        "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
        "filetypes": ["haskell", "lhaskell"],
        // Settings are optional, here are some example values
        "settings": {
          "haskell": {
            "checkParents": "CheckOnSave",
            "checkProject": true,
            "maxCompletions": 40,
            "formattingProvider": "ormolu",
            "plugin": {
              "stan": { "globalOn": true }
            }
          }
        }
      },
      "godot": {
        "host": "127.0.0.1",
        "filetypes": ["gd", "gdscript", "gdscript3"],
        "port": 6008
        }
    }
}
